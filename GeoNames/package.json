        "http-server": "^14.1.1",
        "jquery": "^3.1.1",
        "jquery-slimscroll": "^1.3.8",
        "knockout": "^3.5.0",
        "laravel-mix": "^6.0.19",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.54.5",
        "sass-loader": "^13.0.0",
        "toastr": "^2.1.2"
    },
    "dependencies": {
        "@fontsource/open-sans": "^4.5.11"
    }
